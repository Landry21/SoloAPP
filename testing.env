# Testing Environment Variables
# Use these for your testing environment

# Django Core
SECRET_KEY=test-secret-key-for-testing-only
DEBUG=True
ENVIRONMENT=testing

# Database (Railway will provide this automatically)
# DATABASE_URL=${{ Postgres.DATABASE_URL }}

# Security & CORS (testing domains)
CORS_ALLOWED_ORIGINS=https://localhost:3000,https://localhost:3001,http://localhost:3000,http://localhost:3001
ALLOWED_HOSTS=localhost,127.0.0.1,your-testing-domain.railway.app

# Frontend (testing domains)
REACT_APP_API_BASE_URL=https://your-testing-domain.railway.app/api
REACT_APP_MEDIA_BASE_URL=https://your-testing-domain.railway.app
